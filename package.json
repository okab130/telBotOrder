{
  "name": "tel-bot-order",
  "version": "1.0.0",
  "private": true,
  "description": "Telegram-based mobile order system for restaurants",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "keywords": [
    "telegram",
    "mini-apps",
    "restaurant",
    "order-system",
    "qr-code"
  ],
  "author": "",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*",
    "server"
  ],
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "turbo": "^1.11.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "npm@10.2.4"
}
